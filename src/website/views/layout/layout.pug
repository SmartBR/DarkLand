head
    meta(charset="utf-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1")

    meta(name="theme-color" content="#3d7af5")
    meta(name="author" content="Smart")
    meta(name="description" content="A melhor bot list brasileira de todos os tempos. Venha participar desta conosco.")
    meta(name="keywords" content="dark land bot list discord, bots, lista, melhores, brasileiros, português, música, economia, moderação, vector, vectorbot painel, bots para discord")
    meta(property="og:keywords" content="dark land bot list discord, bots, lista, melhores, brasileiros, português, música, economia, moderação, vector, vectorbot painel, bots para discord")
    meta(property="og:site_name" content="DarkLand")
    meta(property="og:image" content="/img/favicon.png")
    link(rel="icon" href="/img/favicon.png" type="image/png" sizes="128x128")

    script(src="http://code.jquery.com/jquery-latest.js")

    //bootstrap
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.5.4/dist/umd/popper.min.js" integrity="sha384-q2kxQ16AaE6UbzuKqyBE9/u/KzioAlnx2maXQHiDX9d4/zp8Ok3f+M7DPm+Ib6IU" crossorigin="anonymous")
    script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.min.js" integrity="sha384-pQQkAEnwaBkjpqZ8RU1fF1AKtTcHJwFl3pblpTlHXybJjHpMYo79HY3hIi4NKxyj" crossorigin="anonymous")

    //mdbootstrap
    link(rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css")
    link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap")
    link(href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/css/bootstrap.min.css" rel="stylesheet")
    link(href="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/css/mdb.min.css" rel="stylesheet")

    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.4/umd/popper.min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.0/js/bootstrap.min.js")
    script(type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.19.1/js/mdb.min.js")

    script(src="/jquery/formpost.js")

    //toastr
    link(rel="stylesheet" href="/css/toastr.css")
    script(src="/js/toastr.min.js")

    link(rel="stylesheet" href="/css/markdown.css")
    link(rel="text/html" href="/css/mdb.css")
    script(src="/jquery/redefineimage.js")

    block head

body(style="background-color: #383636;")
    nav.navbar.navbar-expand-lg.navbar-dark.nav.dark.mb-4
        a.navbar-brand.font-weight-bold(href="/") DarkLand
        button.navbar-toggler(type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation")
            span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
            ul.navbar-nav.mr-auto.text-center
                li.nav-item
                    a.nav-link(href="/") Início
                li.nav-item
                    a.nav-link(href="/explore") Explorar
                li.nav-item.dropdown
                    a.nav-link.dropdown-toggle(data-toggle="dropdown" aria-expanded="false") Documentação
                    .dropdown-menu.dropdown-menu-right
                        a.dropdown-item(href="/doc/api") API
                li.nav-item
                    a.nav-link(href="/support" target="_blank") Suporte

            form.form-inline.md-form.text-white.my-0(action="/bot")
                i.fas.fa-search(aria-hidden="true")
                input.form-control.ml-3.w-75(type="text" name="search" placeholder="Procurar..." aria-label="Procurar..." style="color: white;" minlength="1" maxlength="30" required value=search || "")

            ul.navbar-nav.nav-flex-icons.text-center
                li.nav-item
                    if (userSession)
                        .dropdown
                            a.nav-link.dropdown-toggle(data-toggle="dropdown" aria-expanded="false")
                                img.rounded-circle(src=`https://cdn.discordapp.com/avatars/${userSession.id}/${userSession.avatar}?size=2048` alt="Avatar" height="35")
                                span(style="margin-left: 5px;") #{userSession.username}
                            .dropdown-menu.dropdown-menu-right
                                a.dropdown-item(href="/dashboard") Painel de Controle
                                if (userSession.admin)
                                    a.dropdown-item(href="/admin") Admin
                                .dropdown-divider
                                a.dropdown-item.red-text(href="/oauth2/logout") Sair
                    else
                        a.nav-link(href="/oauth2/login") Autenticar-se
    block body